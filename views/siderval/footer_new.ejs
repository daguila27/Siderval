                    $("#pedd").addClass('active');
                    $("#page-wrapper").fadeIn();
                }
            });
        });

        function ajax(yo){
        $(".active").removeClass('active');
        $(yo).addClass('active');
        $.ajax({
            type: 'GET',
            url: $(yo).data('link'),
            beforeSend: function(){
                $("#page-wrapper").css('display', 'none');
            },
            success: function(data){
                $("#page-wrapper").html(data);
                $("#page-wrapper").fadeIn();
            }
        });
    }

