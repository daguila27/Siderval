    function show_user(usename){
        if(username === 'calidad'){
            return "Control de Calidad";
        }
    }

    function verificarPantalla(){
        var ancho = 800;
        if( ancho <= 320 ){
            console.log("XXS");
            return '1';
        }
        else if(ancho > 320 && ancho <= 375 ){
            console.log("XS");
            return '2';

        }
        else if(ancho > 375 && ancho <= 425 ){
            console.log("S");
            return '3';

        }
        else if(ancho > 425 && ancho <= 768 ){
            console.log("M");
            return '4';

        }
        else if(ancho > 768 && ancho <= 1024 ){
            console.log("L");
            return '5';

        }
        else if(ancho > 1024 && ancho <= 1440 ){
            console.log("XL");
            return '6';

        }
        else{
            console.log("XXL");
            return '7';
        }
    }
    %>

