<%- include layout/header.ejs %>

<%- include layout/footer.ejs %>

<script>
    //llama a la primera vista
    $.ajax({
        type: 'GET',
        url: '<%= route%>',
        beforeSend: function(){
            showLoad();
        },
        success: function(data){
            setTimeout(function(){
                $("#page-wrapper").data('view', '<%= route%>');
                $("#page-wrapper").html(data);
                hideLoad();
            }, 500);
        }
    });
</script>