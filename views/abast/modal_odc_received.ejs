    <div class="modal-header">
       	<button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4 class="modal-title">Orden de compra: #<%=data[0].numoda%></h4>
    </div>
      <div class="modal-body">
      			<h5><b>Proveedor:</b> <%= data[0].sigla%> - <%= data[0].razon%></h5>
                <table class="table table-striped table-hover table-bordered">
                    <thead>
                    <tr>
                        <th>Nombre</th>
                        <th>Pendientes</th>
                        <th>Recibidos</th>
                        <th>Unidades</th>
                    </tr>
                    </thead>
                    <tbody id="t<%= data[0].idoda%>">
                    <% 
                    if(data[0].matd){
                        data[0].idmat = data[0].idmat.split(',');
                        data[0].matd = data[0].matd.split('@');
                        data[0].xrec = data[0].xrec.split(',');
                        data[0].cant = data[0].cant.split(',');
                        data[0].umed = data[0].umed.split(',');
                        for(var j = 0;j< data[0].matd.length;j++){%>
                            <tr>
                                <!--<td><input type="checkbox" name="hab" onchange="habInput(this)"></td>-->
                                <td style="padding: 5px;"><%= data[0].matd[j].substring(0,1).toUpperCase() + data[0].matd[j].substring(1,data[0].matd[j].lenght).toLowerCase()%></td>
                                <td style="text-align: center;"><%= data[0].xrec[j]%></td>
                                <td style="padding:2px;">
                                    <div class="form-group">
                                        <input type="number" class="form-control" name="rec" value="0" max="<%= data[0].xrec[j]%>" required>
                                    </div>
                                </td>
                                <td style="text-align: center;"><%= data[0].umed[j]%></td>

                                <input type="hidden" name="idoda" class="input-tr" value="<%=data[0].idoda%>">
                                <input type="hidden" name="idmat" class="input-tr" value="<%=data[0].idmat[j]%>">
                                <input type="hidden" name="mat"   class="input-tr" value="<%=data[0].matd[j]%>">
                                <input type="hidden" name="uni"   class="input-tr" value="<%=data[0].umed[j]%>">
                                <input type="hidden" name="cant"  class="input-tr" value="<%=data[0].xrec[j]%>">
                            </tr>
                        <%}
                    }%>

                    </tbody>
      
                </table>
                <div class="input-group">
                    <span class="input-group-addon">NÂ° GD</span>
                    <input type="number" class="form-control" name="numgdd" required>
                </div>

            </div>
    <div class="modal-footer">
        <button type="submit" class="btn btn-primary">He recibido</button>
   		<button type="button" class="btn btn-default" data-dismiss="modal">Cerrar</button>
	</div>