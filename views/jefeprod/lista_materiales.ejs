<%if(data.length>0){%>
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Materiales</th>
                <th>Stock</th>
                <th>Cantidad Total</th>
            </tr>
        </thead>
        <tbody>
        <%
        for(var i=0; i< data.length; i++){
        %>
        <tr>
            <td><%= data[i][3] %></td>
            <td class="parsear_nro"><%= data[i][4] %></td>
            <td class="parsear_nro"><%= data[i][5] %></td>
        </tr>
        <%}%>
        </tbody>
    </table>
<%}else{%>
    <table class="table table-striped">
        <thead>
        <tr>
            <th>Materiales</th>
            <th>Stock</th>
            <th>Cantidad Total</th>
        </tr>
        </thead>
        <tbody>
        <tr>
            <td>No hay materiales a√±adidos</td>
            <td></td>
            <td></td>
        </tr>
        </tbody>
    </table>
<%}%>

